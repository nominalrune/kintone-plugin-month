{
  "name": "kintone-plugin-slider",
  "version": "1.0.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.ts",
    "init": "run-s merge-manifest:prod create-ppk move-ppk",
    "bundle:dev": "webpack --config webpack.dev.js",
    "bundle:prod": "webpack --config webpack.prod.js",
    "bundle:watch": "webpack --config webpack.dev.js -w",
    "merge-manifest:dev": "ts-node scripts/marge-json.ts dev",
    "merge-manifest:prod": "ts-node scripts/marge-json.ts prod",
    "develop": "run-s merge-manifest:dev bundle:dev",
    "build": "run-s  merge-manifest:prod bundle:prod",
    "watch": "run-s merge-manifest:dev bundle:watch",
    "upload": "ts-node scripts/upload.ts",
    "start": "run-p watch upload",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@kintone/dts-gen": "8.0.5",
    "@kintone/plugin-packer": "8.0.4",
    "@kintone/plugin-uploader": "9.0.3",
    "@kintone/rest-api-client": "5.0.7",
    "@kintone/webpack-plugin-kintone-plugin": "8.0.5",
    "@mui/icons-material": "5.15.4",
    "@mui/material": "5.15.4",
    "@types/luxon": "3.4.2",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "dotenv": "16.3.2",
    "esbuild-loader": "4.0.2",
    "eslint": "8.56.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "immer": "10.0.3",
    "luxon": "3.4.4",
    "notistack": "3.0.1",
    "npm-run-all2": "5.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.12",
    "recoil": "0.7.7",
    "terser-webpack-plugin": "5.3.10",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-merge": "5.10.0"
  }
}
